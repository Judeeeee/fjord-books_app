<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<table>
  <thead>
    <tr>
      <th><%= User.human_attribute_name(:name) %></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= link_to "詳細ページへ", user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @users %>
